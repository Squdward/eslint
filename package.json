{
  "name": "@squdward/eslint",
  "version": "1.2.2",
  "description": "My own Eslint config",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:test": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "lint:css": "stylelint ./**/*.{css,scss}",
    "lint:css:fix": "stylelint --fix  ./**/*.{css,scss}",
    "format": "prettier --write .",
    "check-format": "prettier --check ."
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-preset",
    "code-quality",
    "linting",
    "prettier",
    "stylelint",
    "typescript",
    "react",
    "javascript",
    "coding-style",
    "best-practices"
  ],
  "author": "Ilya Politik",
  "license": "ISC",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./prettier": {
      "import": "./prettier.js"
    },
    "./stylelint": {
      "import": "./stylelint.js"
    },
    "./configs/react": {
      "import": "./configs/react.js"
    },
    "./configs/typescript": {
      "import": "./configs/typescript.js"
    },
    "./main": {
      "import": "./main.js"
    }
  },
  "dependencies": {
    "@eslint/js": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.11.1",
    "typescript-eslint": "^8.32.0"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "main.config.js",
    "prettier.js",
    "stylelint.js",
    "configs/react.js",
    "configs/typescript.js"
  ],
  "peerDependencies": {
    "eslint": ">=9.26.0",
    "prettier": ">=3.5.3",
    "stylelint": ">=16.19.1",
    "typescript": ">=5.0.0"
  }
}
